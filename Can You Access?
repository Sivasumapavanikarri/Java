import java.io.*;
import java.lang.reflect.*;

public class Solution {

    public static void main(String[] args) throws Exception {
        BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
        int num = Integer.parseInt(br.readLine().trim());

        Inner inner = new Inner();
        Inner.Private obj = inner.new Private();

        Method m = Inner.Private.class.getDeclaredMethod("powerOfTwo", int.class);
        m.setAccessible(true);

        String result = (String) m.invoke(obj, num);

        System.out.println(num + " is " + result);
        System.out.println("An instance of class: " + obj.getClass().getCanonicalName() 
                + " has been created");
    }

    static class Inner {
        private class Private {
            private String powerOfTwo(int num) {
                return ((num & (num - 1)) == 0) ? "power of 2" : "not a power of 2";
            }
        }
    }
}
